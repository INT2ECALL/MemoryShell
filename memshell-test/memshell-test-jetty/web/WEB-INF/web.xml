<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>org.su18.memshell.test.jetty.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddJettyServlet</servlet-name>
        <servlet-class>org.su18.memshell.test.jetty.AddJettyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddJettyFilter</servlet-name>
        <servlet-class>org.su18.memshell.test.jetty.AddJettyFilter</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddJettyServlet</servlet-name>
        <url-pattern>/addJettyServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddJettyFilter</servlet-name>
        <url-pattern>/addJettyFilter</url-pattern>
    </servlet-mapping>
</web-app>