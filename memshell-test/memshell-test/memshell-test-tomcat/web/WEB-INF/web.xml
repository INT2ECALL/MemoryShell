<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>org.su18.memshell.test.tomcat.IndexServlet</servlet-class>
    </servlet>


    <servlet>
        <servlet-name>QuertStringServlet</servlet-name>
        <servlet-class>org.su18.memshell.test.tomcat.QueryStringServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>QuertStringServlet</servlet-name>
        <url-pattern>/query</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddTomcatListener</servlet-name>
        <servlet-class>org.su18.memshell.test.tomcat.AddTomcatListener</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AddTomcatFilterServlet</servlet-name>
        <servlet-class>org.su18.memshell.test.tomcat.AddTomcatFilter</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AddTomcatServlet</servlet-name>
        <servlet-class>org.su18.memshell.test.tomcat.AddTomcatServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>AddTomcatValve</servlet-name>
        <servlet-class>org.su18.memshell.test.tomcat.AddTomcatValve</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddTomcatFilterServlet</servlet-name>
        <url-pattern>/addTomcatFilter</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddTomcatServlet</servlet-name>
        <url-pattern>/addTomcatServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddTomcatListener</servlet-name>
        <url-pattern>/addTomcatListener</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>AddTomcatValve</servlet-name>
        <url-pattern>/addTomcatValve</url-pattern>
    </servlet-mapping>


</web-app>